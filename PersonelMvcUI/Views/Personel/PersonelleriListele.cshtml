@model  IEnumerable< PersonelMvcUI.Models.EntityFramework.Personel>


<table class="table table-bordered">
    <thead>
        <tr>
            <th>Ad</th>
            <th>Soyad</th>
            <th>Doğum Tarihi</th>
            <th>Maaş</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var item in Model)
        {
            <tr>
                <td>@item.Ad</td>
                <td>@item.Soyad</td>
                <td>@item.DogumTarih.Value.ToShortDateString()</td>
                <td>@item.Maas</td>
            </tr>
        }

    </tbody>
</table>
